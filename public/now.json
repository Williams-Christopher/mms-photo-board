{
    "version": 2,
    "name": "mms-photo-board",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/favicon.ico$",
          "dest": "/favicon.ico"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }
